        },
        {
            "name": "edm4hep",
            "description": "Event data model of Key4hep."
        },
        {
            "name": "gaudi",
            "description": "An experiment-independent HEP event data processing framework"
        },
        {
            "name": "hermes",
            "description": "Hermes is a heterogeneous-aware, multi-tiered, dynamic, and distributed\nI/O buffering system that aims to significantly accelerate I/O\nperformance."
        },
        {
            "name": "autodiff",
            "description": "autodiff is automatic differentiation made easier for C++."
        },
        {
            "name": "dihydrogen",
            "description": "DiHydrogen is the second version of the Hydrogen fork of the well-known\ndistributed linear algebra library, Elemental. DiHydrogen aims to be a\nbasic distributed multilinear algebra interface with a particular\nemphasis on the needs of the distributed machine learning effort, LBANN."
        }
